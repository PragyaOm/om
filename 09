#include<stdio.h>
#include<maths.h>
int main()
{
    int a;
    printf("enter the int number :");
    scanf("%d",&a);
    printf("%d\n",a);
    
    floatb;
    printf("enter float number:");
    scanf("%f",&b);
    printf("%f\n",b);
    
    char c;
    printf("enter the character:");
    scanf("%s"&c);
    printf("%c\n",c);
    
    return 0;
}
