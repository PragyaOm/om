#include<stdio.h>
int main()
{
    int a[1000],i,n;
    printf("enter the size of array:");
    scanf("%d",&n);
    printf("enter %d elements in the arry:"n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("\nelements in array are:");
    for(i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
    return 0;
}